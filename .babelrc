{
    "presets": [
        "react",
        "es2015",
        "stage-0",
        "stage-1",
        "stage-2"

    ],
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers":true,
                "polyfill": true,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ],
        [
            "import",
            {
                "libraryName": "antd",
                "style": false
            }
        ]
    ]
}